{
  "name": "No Blocking Enchanting Table",
  "description": "Place anything next to your enchanting table!",
  "author": "Olillin",
  "version": "1.1.0",
  "data_pack": {
    "name": "no-blocking-enchanting-table-{{ project_version }}",
    "description": "{{ project_description }}",
    "pack_format": 57,
    "supported_formats": [
      16,
      57
    ],
    "overlays": [
      {
        "formats": {
          "min_inclusive": 43,
          "max_inclusive": 57
        },
        "directory": "post_1_21"
      },
      {
        "formats": {
          "min_inclusive": 16,
          "max_inclusive": 42
        },
        "directory": "pre_1_21"
      }
    ],
    "load": [
      "src"
    ],
    "zipped": true
  },
  "output": "build",
  "pipeline": [
    "not_required",
    "beet.contrib.minify_json",
    "fix_paths"
  ]
}